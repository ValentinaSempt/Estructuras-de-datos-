# ejemplo de uso de colas
fruits=[]
pilas=[]

# incluyendo elementos push

fruits.append("banana")
fruits.append("grapes")
fruits.append("mango")
fruits.append("orange")

# sacando elementos

fruits_item=fruits.pop(0)
# es el primer elemento de una lista 
pilas.append(fruits_item)

# nuevamente

fruits_item= fruits.pop(0)
pilas.append(fruits_item)

fruits_item= fruits.pop(0)
pilas.append(fruits_item)

print("fruits: ", fruits)
print("pilas", pilas)

# pop 0 en caso de cola y pop en caso de pila
